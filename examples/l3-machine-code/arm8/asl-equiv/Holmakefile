MODEL=mra-hol-snapshots
MODEL_HEAP=mra-hol-snapshots/A64_ISA_v86A/armv86a-heap
PROOFS_HEAP=proofs/l3-equivalence-heap

.PHONY: all
all: $(PROOFS_HEAP)

$(MODEL):
	./get-mra-hol-snapshots

